QMenuBar {
    border: 0;
}

QToolTip {
    padding: 3px;
}

QListWidget {
    padding: 3px;
}

QListWidget::item {
    padding: 6px;
}

QComboBox::item {
    height: 16px;
    padding: 6px;
}

QComboBox::item::selected {
    background-color: #1464A0;
    color: #F0F0F0;
}

#labelImageInfo {
    padding: 6px;
    border: 1px solid #32414B;
    border-radius: 3px;
}

#labelDepthLabel,
#labelFileSizeLabel,
#labelImageSizeLabel {
    padding: 6px;
}

#ImageInfoDialog #labelImageSizeLabel,
#ImageInfoDialog #labelDPILabel,
#ImageInfoDialog #labelFileSizeLabel,
#ImageInfoDialog #labelDepthLabel,
#ImageInfoDialog #labelPaletteSizeLabel,
#ImageInfoDialog #labelOrientationLabel,
#ImageInfoDialog #labelICCProfileLabel,
#ImageInfoDialog #labelDataTypeLabel {
    border-radius: 3px;
    padding: 6px;
