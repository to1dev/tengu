    border: 0;
    background-color: #263849;
}

#ButtonRunGroup:pressed, #ButtonBindGroup:pressed, #ButtonReleaseGroup:pressed, #ButtonBindProfile:pressed, #ButtonBindLayout:pressed {
    background-color: #34495E;
}

#ButtonRunGroup:disabled, #ButtonBindGroup:disabled, #ButtonReleaseGroup:disabled, #ButtonBindProfile:disabled, #ButtonBindLayout:disabled {
    color: #787878;
    background-color: #454545;
}

#labelGroup, #labelProcess {
    color: #aaaaaa;
    padding: 6;
    border: 1 solid #32414b;
    border-radius: 0;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(20, 20, 20, 150), stop:0.2 palette(window));
}

QTextEdit {
    padding: 9;
