--------------------------------------------------------------------------- */

#ButtonStart, #ButtonStop, #ButtonSettings, #ButtonBuy, #ButtonContact {
    border-radius: 0;
    border: 1 solid #32414b;
    background: transparent;
    width: 60px;
    height: 60px;
    padding: 8px;
}

#ButtonStart:pressed {
    padding: 9px;
}

#ButtonStart:hover, #ButtonStop:hover, #ButtonSettings:hover, #ButtonBuy:hover, #ButtonContact:hover {
    border-color: palette(highlight);
}

#ButtonLeftTop, #ButtonLeftBottom, #ButtonRightTop, #ButtonRightBottom {
    color: #aaaaaa;
    padding: 0;
    border-radius: 0;
    border: 1 solid #32414b;
    width: 12px;
    height: 12px;
}

#ButtonLeftTop:hover, #ButtonLeftBottom:hover, #ButtonRightTop:hover, #ButtonRightBottom:hover {
    border-color: palette(highlight);
}

#ButtonMain, #buttonMainMenu, #ButtonGroup, #ButtonProcess, #ButtonGold, #ButtonMacro, #ButtonAddon {
    color: #aaaaaa;
    padding: 6;
    border-radius: 0;
    border: 1 solid #32414b;
    width: 20;
    height: 20;
}

#ButtonMain:hover, #buttonMainMenu:hover, #ButtonGroup:hover, #ButtonProcess:hover, #ButtonGold:hover, #ButtonMacro:hover, #ButtonAddon:hover {
    border-color: palette(highlight);
}

#ButtonGroup:checked, #ButtonProcess:checked {
    border-color: palette(highlight);
}

#ButtonMin, #ButtonMax, #ButtonClose {
    color: #ffffff;
    background: transparent;
    padding: 1;
    border-radius: 0;
    border: 1 solid #32414b;
    min-width: 24px;
    min-height: 24px;
    height: 24px;
    width: 24px;
}

#ButtonMin:hover, #ButtonMax:hover, #ButtonClose:hover {
    border: 1 solid palette(highlight);
}

#ButtonMin:pressed, #ButtonMax:pressed, #ButtonClose:pressed {
}

#frameMain {
    background: rgba(125, 135, 145, 200);
    border-radius: 6;
    border-top-left-radius: 6;
    border-top-right-radius: 6;
    border: 0;
    border: 2 solid #111111;
}

#frameContent {
    border-radius: 0;
}

#labelStatus {
    padding: 6;
    color: #aaaaaa;
    border: 1 solid #32414b;
    background-origin: content;
    background: rgba(25, 35, 45, 200);
}

#titleBar {
    min-height: 50;
    border-radius: 0;
    border-top-left-radius: 6;
    border-top-right-radius: 6;
    /*border-bottom: 3 solid palette(highlight);*/
}

#dragBar {
    border-top-left-radius: 0;
    border-top-right-radius: 0;
}

#frameTop {
    border-radius: 0;
}

#labelTitle {
    font-family: "Edu NSW ACT Foundation";
    font-weight: 700;
    border-top-right-radius: 0;
}

#labelIcon {
    border-top-left-radius: 0;
}

#listWidgetGroups, #listWidgetProcesses {
    border-radius: 0;
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 rgba(20, 20, 20, 150), stop:.03 palette(window));
}

#listWidgetGroups::item {
    border-radius: 6;
    width: 100;
    height: 120;
}

QScrollBar:horizontal {
    border-radius: 0;
    border: 0;
}

QScrollBar::handle:horizontal {
    border-radius: 2;
    border: 0;
    background: #174c6d;
}

QScrollBar::handle:horizontal:hover {
    border-radius: 2;
    border: 0;
    background: #174c6d;
}

QScrollBar:vertical {
    border-radius: 0;
    border: 0;
}

QScrollBar::handle:vertical {
    border-radius: 2;
    border: 0;
    background: #174c6d;
}

QScrollBar::handle:vertical:hover {
    border-radius: 2;
    border: 0;
    background: #174c6d;
}

#ButtonNewGroup, #ButtonEditGroup, #ButtonDeleteGroup, #ButtonRefreshProcess, #ButtonSetLeader, #ButtonChangeTitle {
    border-radius: 15;
    border: 0;
    background-color: #1ABC9C;
    color: white;
    width: 90;
    height: 30;
}

#ButtonNewGroup:hover, #ButtonEditGroup:hover, #ButtonDeleteGroup:hover, #ButtonRefreshProcess:hover, #ButtonSetLeader:hover, #ButtonChangeTitle:hover {
    border: 0;
    background-color: #12AB8D;
}

#ButtonNewGroup:pressed, #ButtonEditGroup:pressed, #ButtonDeleteGroup:pressed, #ButtonRefreshProcess:pressed, #ButtonSetLeader:pressed, #ButtonChangeTitle:pressed {
    background-color: #1ABC9C;
}

#ButtonNewGroup:disabled, #ButtonEditGroup:disabled, #ButtonDeleteGroup:disabled, #ButtonRefreshProcess:disabled, #ButtonSetLeader:disabled, #ButtonChangeTitle:disabled {
    color: #787878;
    background-color: #454545;
}

#ButtonRunGroup, #ButtonBindGroup, #ButtonReleaseGroup, #ButtonBindProfile, #ButtonBindLayout {
    border-radius: 15;
    border: 0;
    background-color: #34495E;
    color: white;
    width: 90;
    height: 30;
}

