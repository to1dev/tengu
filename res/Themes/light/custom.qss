    border-radius: 0;
}

QComboBox {
    border-radius: 0;
}

QComboBox:hover, QComboBox:focus {
    border-color: #32414b;
}

QComboBox::item:checked {
    font-weight: normal;
}

QComboBox::drop-down {
    width: 20px;
}

QComboBox::icon {
    margin-left: 4px;
}

QComboBox::indicator {
    margin-left: 4px;
}

QComboBox::indicator:checked {
    image: url(:/Media/Check);
}

