}

#ImageInfoDialog #labelImageSize,
#ImageInfoDialog #labelDPI,
#ImageInfoDialog #labelFileSize,
#ImageInfoDialog #labelDepth,
#ImageInfoDialog #labelPaletteSize,
#ImageInfoDialog #labelOrientation,
#ImageInfoDialog #labelICCProfile,
